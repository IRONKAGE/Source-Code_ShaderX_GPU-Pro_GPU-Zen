float4x4 matBackdropWorld;

struct VS_OUTPUT
{
   float4 Pos:       POSITION;
   float2 Tex:       TEXCOORD0;
   float4 VtxPos:    TEXCOORD1;
};

VS_OUTPUT main(
   float4 vPosition: POSITION,
   float3 vNormal:   NORMAL,
   float2 vTex:      TEXCOORD )
{
   VS_OUTPUT Out = (VS_OUTPUT) 0;
   
   float4x4 matWorldViewProjection = mul( matViewProjection, matBackdropWorld );
   
   Out.Pos = mul( matWorldViewProjection, vPosition );
   Out.Tex = float2( vTex.x, -vTex.y );
   Out.VtxPos = mul( matBackdropWorld, vPosition );

   return Out;
}


