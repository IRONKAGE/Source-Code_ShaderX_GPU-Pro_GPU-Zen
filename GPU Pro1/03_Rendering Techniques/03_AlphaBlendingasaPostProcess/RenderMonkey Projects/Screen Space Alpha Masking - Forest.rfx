    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[

struct VS_OUTPUT
{
    float4 Pos :     POSITION;
    half3  Diffuse : COLOR0;
    half2  Tex :     TEXCOORD0;
    float4 VtxPos :  TEXCOORD1;
};

float4x4 matViewProjection;
float4x4 matTreeWorld;

float4 lightAmbient;
float4 lightSky;
float4 lightGround;

float3 sunDirection;
float4 sunColor;

float3 kickerDirection;
float4 kickerColor;

VS_OUTPUT main( float4 vPosition : POSITION,
                float3 vNormal :   NORMAL,
                float2 vTex :      TEXCOORD )
{
    VS_OUTPUT Out = (VS_OUTPUT) 0;
   
    float3 sunDir = normalize( mul( matTreeWorld, float4( sunDirection, 0.0f ) ) );
    float3 kickerDir = normalize( mul( matTreeWorld, float4( kickerDirection, 0.0f ) ) );
   
    float4x4 matWorldViewProjection = mul(matViewProjection, matTreeWorld);
   
    Out.Pos = mul(matWorldViewProjection, vPosition);
    Out.Tex = float2( vTex.x, -vTex.y );
   
    half3 normal = normalize( mul(matTreeWorld, float4(vNormal, 0.0)) );

    half upness = 0.5f + normal.y * 0.5f;

    // hemisphere ambient term
    half3 rgb = lerp( lightGround, lightSky, upness ) + lightAmbient;
    
    // two diffuse lights
    half d1 = dot( normal, sunDir );
    rgb += saturate(d1) * sunColor;

    half d2 = dot( normal, kickerDir );
    rgb += saturate(d2) * kickerColor;
    
    Out.Diffuse = rgb;
    
    Out.VtxPos = mul( matTreeWorld, vPosition );
    
    return Out;
}

]]></RmShaderCode>
