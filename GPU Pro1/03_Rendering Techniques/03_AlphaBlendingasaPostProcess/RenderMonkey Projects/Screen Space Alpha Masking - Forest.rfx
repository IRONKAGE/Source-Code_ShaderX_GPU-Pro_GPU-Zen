    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[

sampler2D BaseMap;
sampler2D DetailMap1;
sampler2D DetailMap2;
sampler2D LightMap;
sampler2D BlendMap;
sampler2D CloudMap;

float  fTime;
float4 vViewPosition;

float4 sunColor;
float4 skyColor;
float  sunStrength;

float  cloudRange;

float  detailRange;
float  detailScale;

struct PS_INPUT 
{
    float4 Pos :    POSITION;
    float2 Tex :    TEXCOORD0;
    float4 VtxPos : TEXCOORD1;
};

half4 main( PS_INPUT In ) : COLOR 
{
    float2 detailUV = In.Tex * detailScale;
    
    half4 baseTex    = tex2D( BaseMap,    In.Tex );
    half4 lightTex   = tex2D( LightMap,   In.Tex );
    half4 blendTex   = tex2D( BlendMap,   In.Tex );
    half4 detailTex1 = tex2D( DetailMap1, detailUV );
    half4 detailTex2 = tex2D( DetailMap2, detailUV );

    float cloudBlend = 1 - saturate( length(In.VtxPos.xyz - vViewPosition.xyz) * cloudRange );
    half4 cloudTex1  = tex2D( CloudMap, float2(fTime * -0.2f, fTime * -0.2f) + In.VtxPos.xz * float2(0.002f, 0.002f) );
    half4 cloudTex2  = 1 - tex2D( CloudMap, float2(fTime * -0.3f, fTime * -0.3f) + In.VtxPos.xz * float2(0.002f, 0.002f) );
    half  cloudCover = 1 - ( 0.5h * saturate( 0.8h + cloudTex1.z - cloudTex2.z ) );
          cloudCover = lerp( half4(1,1,1,1), cloudCover, saturate(8*cloudBlend) );
    
    float detailBlend = 1 - saturate( length(In.VtxPos.xyz - vViewPosition.xyz) * detailRange );

    return baseTex * sunColor * cloudCover * sunStrength * saturate(detailBlend + 0.5h)
         * lerp( half4(1,1,1,1), lerp(detailTex2, detailTex1, blendTex), saturate( 0.8h * detailBlend ) ) 
         * lerp( skyColor * lightTex, half4(1,1,1,1), lightTex * lightTex );
}
]]></RmShaderCode>
