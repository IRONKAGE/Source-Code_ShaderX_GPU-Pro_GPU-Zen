sampler2D diffuseMap: register(s0);



struct PS_INPUT
{
	float4 color: COLOR;
	float2 texCoord0: TEXCOORD0;
};



struct PS_OUTPUT
{
	float4 color: COLOR;
};



PS_OUTPUT main(PS_INPUT input)
{
	PS_OUTPUT output;

	output.color = input.color * tex2D(diffuseMap, input.texCoord0);

	return output;
}
